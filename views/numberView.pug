html
  head
    meta(charset='UTF-8')
    meta(name='viewport', content='width=device-width, initial-scale=1.0')
    title Game
    link(rel='stylesheet', href='css\bootstrap.min.css')
  body
    .box-in
      h1.rank You are number...
      h1.no-1 1
    a(href='index.html')
      button.exit Exit
    style.
      body{
      background: #DDD4A2;
      position: relative;
      width: 1980px;
      height: 1080px;
      }
      .box-in{
      position: absolute;
      width: 1230px;
      height: 800px;
      left: 345px;
      top: 78px;
      align-items: center;
      text-align: center;
      background: #E5E5E5;
      border-radius: 73px;
      }
      .rank{
      position: absolute;
      width: 1000px;
      height: 39px;
      left: 110px;
      top: 200px;
      font-family: 'Inter';
      font-style: normal;
      font-weight: 700;
      font-size: 100px;
      line-height: 39px;
      /* identical to box height */
      text-align: center;
      color: #303030;
      }
      .no-1{
      position: absolute;
      width: 900px;
      height: 155px;
      left: 182px;
      top: 450px;
      font-family: 'Inter';
      font-style: normal;
      font-weight: 800;
      font-size: 400px;
      line-height: 155px;
      /* identical to box height */
      text-align: center;
      color: #333333;
      text-shadow: 0px 6px 4px rgba(0, 0, 0, 0.25);
      }
      .exit{
      position: absolute;
      top:920px;
      left: 1650px;
      width: 350px;
      height: 140px;
      background: rgba(120, 120, 120, 1);
      box-shadow: 0px 5px 4px rgba(0, 0, 0, 0.25);
      border-radius: 50px;
      font-family: 'Inter';
      font-style: normal;
      font-weight: 700;
      font-size:70px;
      line-height: 29px;
      display: flex;
      align-items: center;
      text-align: center;
      padding-left: 110px;
      color: #FFFFFF;
      border: 0px;
      }
